generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Beer {
  id                 String   @id
  styleName          String
  minimumTemperature String
  maximumTemperature String
  createdAt          DateTime @default(now())
}
